script/source = "@tool

extends TextureRect

@onready var camera: FreeCamera3D = %FreeCamera3D


var frame: float = 0

func _ready():
    resized.connect(func():
        frame = 0
    )


func _process(_delta: float) -> void:
    if is_instance_valid(camera):
        if camera.moving:
            frame = 0
    if Input.is_action_pressed(\"ui_accept\"):
        frame = 0

    frame += 1

    material.set_shader_parameter(\"frame\", frame)
"

